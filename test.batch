#!/bin/bash

#SBATCH --job-name FireHose
#SBATCH --ipus=1
#SBATCH --partition=p64
#SBATCH --nodelist=gc-poplar-03
#SBATCH --ntasks 1
#SBATCH --time=00:05:00
#SBATCH --output=tensor_decomp_test_%j.log

echo "STARTING\n"

for value in {1..3}
do
    srun ./firehose
    make clean
    sleep 2m
done

echo "DONE\n"